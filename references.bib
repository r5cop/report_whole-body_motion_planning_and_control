	Seoul, 151-742, South Korea; email: yykim@snu.ac.kr},
  file = {:G\:\\PhD Project\\Research Papers\\Journal Articles\\Ryu_2011.pdf:PDF},
  issn = {1615147X (ISSN)},
  journal = {Struct. Mutltidiscip. Opt.},
  keywords = {Conduction heat flow, Mobile robot, Path planning, Topology optimization},
  owner = {JLunenburg},
  pages = {1--13},
  timestamp = {2012.05.09},
