	di Tecnologia, Genoa, Italy},
  file = {:G\:\\PhD Project\\Research Papers\\Journal Articles\\Mohan_2011.pdf:PDF},
  issn = {16625218 (ISSN)},
  keywords = {Cognitive architecture, Covert actions, Humanoid robots, iCub, Optimal
	control theory, Passive motion paradigm, Synergy formation},
  owner = {JLunenburg},
  timestamp = {2012.08.31}
}

