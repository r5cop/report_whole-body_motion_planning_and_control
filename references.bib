@INPROCEEDINGS{Berenson2008,
  author = {Berenson, D. and Kuffner, J. and Choset, H.},
  title = {An optimization approach to planning for mobile manipulation},
  booktitle = {2008 IEEE International Conference on Robotics and Automation},
  year = {2008},
  pages = {1187--1192},
  abstract = {We present an optimization-based approach to grasping and path planning
	for mobile manipulators. We focus on pick-and-place operations, where
	a given object must be moved from its start configuration to its
	goal configuration by the robot. Given only the start and goal configurations
