@ARTICLE{Dietrich2012,
  author = {Dietrich, A. and Wimb\"{o}ck, T. and Albu-Sch\"{a}ffer, A. and Hirzinger,
	G.},
  title = {Reactive Whole-Body Control: Dynamic Mobile Manipulation Using a
	Large Number of Actuated Degrees of Freedom},
  journal = {IEEE Robotics \& Automation Magazine},
  year = {2012},
  volume = {19},
  pages = {20--33},
  number = {2},
  abstract = {As a result of intensive research over the last few decades, several
	robotic systems are approaching a level of maturity that allows robust
	task execution and safe interaction with humans and the environment.
	Particularly when considering the aging of the population, service
	and household robotics is expected to play an important role in future
	domestic environments. To provide the ability to accomplish a huge
	range of tasks with different requirements, it appears to be inevitable
